.container
  %h1 Ordenes de: #{current_user[:name]}
  %table.table.table-bordered
    %trhead
      %tr
        %th Order ID
        %th User ID
    %tbody
      - @orders.each do |order|
        %tr.clickable-row{"data-href": order_path(order.id)}
          %td= order.id
          %td= order.user_id